{
  "short_name": "TurtleTab",
  "name": "TurtleTab",
  "manifest_version": 1.0,
  "version": "1.0",
  "start_url": "./index.html",
  "display": "standalone",
  "theme_color": "#000000",
  "background_color": "#ffffff",

  "default_locale": "en",
  "description": "Chrome extension with built-in weather, todo, notes, and google search functions",
  "icons": [
      {
        "src": "favicon.ico",
        "sizes": "192x192",
        "type": "image/png"
      }
  ],

  "browser_action": {
    
  },
  "background": {
    "persistent": false,
  },
  "permissions": [
    "activeTab",
  ],

}
